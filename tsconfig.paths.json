{
    "compilerOptions": {
        "baseUrl": "./src",
        "paths": {
            "@routes": [
                "./routes"
            ],
            "@routes/*": [
                "./routes/*"
            ],
            "@utils": [
                "./utils"
            ],
            "@utils/*": [
                "./utils/*"
            ],
            "types/*": [
                "./types/*"
            ],
            "@models": [
                "./models"
            ],
            "@models/*": [
                "./models/*"
            ],
            "@middlewares": [
                "./middlewares"
            ],
            "@middlewares/*": [
                "./middlewares/*"
            ],
            "@api": [
                "./api"
            ],
            "@api/*": [
                "./api/*"
            ],
            "@lib": [
                "./libs"
            ]
        }
    }
}