{
    "compilerOptions": {
        "baseUrl": "./",
        "paths": {
            "@routes": [
                "src/routes"
            ],
            "@routes/*": [
                "src/routes/*"
            ],
            "@utils": [
                "src/utils"
            ],
            "@utils/*": [
                "src/utils/*"
            ],
            "types/*": [
                "src/types/*"
            ],
            "@models": [
                "src/models"
            ],
            "@models/*": [
                "src/models/*"
            ],
            "@middlewares": [
                "src/middlewares"
            ],
            "@middlewares/*": [
                "src/middlewares/*"
            ],
            "@api": [
                "src/api"
            ],
            "@api/*": [
                "src/api/*"
            ]
        }
    }
}