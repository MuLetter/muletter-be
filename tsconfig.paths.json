{
    "compilerOptions": {
        "baseUrl": "./",
        "paths": {
            "@/*": [
                "src/*"
            ],
            "@routes": [
                "src/routes"
            ],
            "@routes/*": [
                "src/routes/*"
            ],
            "@utils": [
                "src/utils"
            ],
            "types/*": [
                "src/types/*"
            ]
        }
    }
}