{
    "compilerOptions": {
        "baseUrl": "./",
        "paths": {
            "@/*": [
                "src/*"
            ],
            "@routes": [
                "src/routes"
            ],
            "@routes/*": [
                "src/routes/*"
            ],
            "@utils": [
                "src/utils"
            ],
            "@utils/*": [
                "src/utils/*"
            ],
            "types/*": [
                "src/types/*"
            ],
            "@models": [
                "src/models"
            ],
            "@models/*": [
                "src/models/*"
            ],
            "@middleware": [
                "src/middlware"
            ],
            "@middleware/*": [
                "src/middleware/*"
            ]
        }
    }
}